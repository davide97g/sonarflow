{
  "name": "@sonarflow/versioning",
  "version": "1.0.0-beta.4",
  "description": "Versioning utilities for Sonarflow",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsgo",
    "dev": "tsgo --watch",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "format": "biome format --write src",
    "check": "biome check src",
    "check:fix": "biome check --write src"
  },
  "dependencies": {
    "@sonarflow/sonar": "workspace:*",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3"
  },
  "files": [
    "dist"
  ]
}

